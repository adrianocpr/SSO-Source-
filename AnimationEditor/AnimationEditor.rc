IDR_MAINFRAME MENU 
BEGIN
    POPUP "文件(&F)"
    BEGIN
        MENUITEM "新建(&N)",                      ID_FILE_NEW
        MENUITEM "打开(&O)",                      ID_FILE_OPEN
        MENUITEM "保存(&S)",                      ID_FILE_SAVE
        MENUITEM "另存为(&A)",                     ID_FILE_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "退出(&Q)",                      ID_APP_EXIT
        MENUITEM "导入动画组",                       ID_IMPORT_ACTION_GROUP
        MENUITEM "导出动画组",                       ID_EXPORT_ACTION_GROUP
        MENUITEM SEPARATOR
        MENUITEM "重新加载音频工程",                    ID_REBUILD_AUDIO
    END
    POPUP "编辑(&E)"
    BEGIN
        MENUITEM "撤消(&U)\tCtrl+Z",              ID_EDIT_UNDO
        MENUITEM "重做(&R)\tCtrl+Y",              ID_EDIT_REDO
        MENUITEM SEPARATOR
        MENUITEM "复制(&C)\tCtrl+C",              ID_EDIT_COPY
        MENUITEM "粘贴(&P)\tCtrl+V",              ID_EDIT_PASTE
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "显示FPS(&F)",                   ID_SHOW_FPS
        MENUITEM "显示AABB(&A)",                  ID_SHOW_AABB
        MENUITEM "线框显示(&W)",                    ID_SHOW_WIRE
        MENUITEM "工具栏(&T)",                     ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                     ID_VIEW_STATUS_BAR
        MENUITEM "对象栏(&O)",                     ID_OBJECT_LIST
        MENUITEM "通道栏(&K)",                     ID_TRACK_PANEL
        MENUITEM "计算动画总时间(&C)",                 ID_CALCULATE_TOTAL_TIME
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 AnimationEditor(&A)...",   ID_APP_ABOUT
    END
END


